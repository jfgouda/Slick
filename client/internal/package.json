{
  "name": "sketch-net",
  "version": "0.0.3-alpha",
  "description": "Sketching tool for a new gas request. Using Angular CLI and Fabric JS",
  "author": "John Hakeem",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "pre-build": "node build/pre-build.js",
    "post-build": "node build/post-build.js",
    "node": "node build/server.js",
    "prod": "ng build --prod",
    "prod-soco": "ng build --prod --base-href /SketchGenerator/",
    "prod-soco-log": "ng build --prod --verbose --progress --named-chunks --base-href /SketchGenerator/",
    "deploy": "firebase deploy",
    "github-build": "ng build --prod --base-href https://jfgouda.github.io/sketch.net/",
    "github-publish": "ngh",
    "workbox": "node build/post-build-workbox.js",
    "debug": "npm run pre-build && npm run start",
    "build": "npm run pre-build && npm run prod && npm run node",
    "build-soco": "npm run pre-build && npm run prod-soco && npm run workbox && npm run post-build",
    "build-soco-log": "npm run pre-build && npm run prod-soco-log && npm run post-build",
    "build-gh": "npm run pre-build && npm run github-build && npm run workbox && npm run github-publish",
    "publish": "npm run pre-build && npm run prod && npm run deploy",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^5.0.4",
    "@angular/common": "^5.0.4",
    "@angular/compiler": "^5.0.4",
    "@angular/core": "^5.0.4",
    "@angular/forms": "^5.0.4",
    "@angular/http": "^5.0.4",
    "@angular/platform-browser": "^5.0.4",
    "@angular/platform-browser-dynamic": "^5.0.4",
    "@angular/router": "^5.0.4",
    "angular-datatables": "^5.0.0",
    "angular2-indexeddb": "^1.2.2",
    "classlist.js": "~1.1.20150312",
    "core-js": "^2.5.1",
    "datatables.net": "^1.10.16",
    "datatables.net-dt": "^1.10.16",
    "fabric": "~2.0.0-rc.3",
    "intl": "~1.2.5",
    "jquery": "~3.2.1",
    "rxjs": "^5.5.2",
    "web-animations-js": "~2.3.1",
    "workbox-background-sync": "^2.0.3",
    "workbox-build": "^2.1.2",
    "workbox-sw": "^2.1.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.0.4",
    "@angular/language-service": "^5.0.4",
    "@types/datatables.net": "^1.10.8",
    "@types/jquery": "^3.2.17",
    "@types/node": "^8.0.53",
    "angular2-indexeddb": "^1.2.2",
    "codelyzer": "~4.0.1",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~2.6.2",
    "webpack-dev-server": "~2.9.5",
    "workbox-broadcast-cache-update": "^2.0.3",
    "workbox-routing": "^2.1.0"
  }
}
